<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="Dashboard">
  <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
  <title>Mobile</title>
  <link type="image/png" href="images/adityalogo.png" rel="icon">
  <!-- Bootstrap core CSS-->
  <link rel="stylesheet" href="stylesheets/bootstrap.min.css">
  <script src="javascripts/jquery-1.11.1.min.js"></script>
  <script src="javascripts/bootstrap.min.js"></script>
  <!-- external css-->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <!-- Custom styles for this template-->
  <link href="stylesheets/style4.css" rel="stylesheet">
  <link href="stylesheets/style-responsive.css" rel="stylesheet">
  <!--Date Picker-->
  <link href="stylesheets/jquery-ui.css" rel="stylesheet">
  <script type="text/javascript" src="angularjs/angular.min.js"></script>
  <script type="text/javascript" src="angularjs/studenthome.js"></script>
  <style>
    .panel-box {
      display: table;
      table-layout: fixed;
      width: 100%;
      height: 30px;
      text-align: center;
      border: medium none;
    }

    .bg-success {
      background-color: #A3C86D !important;
      color: #FFF !important;
    }

    .bg-warning {
      background-color: orange !important;
      color: #FFF !important;
    }

    .bg-light {
      background-color: lightblue !important;
      color: #FFF !important;
    }

    .bg-lightgreen {
      background-color: lightgreen !important;
      color: #FFF !important;
    }

    .bg-default {
      background-color: grey !important;
      color: #FFF !important;
    }

    .bg-pink {
      background-color: pink !important;
      color: #FFF !important;
    }

    .bg-tomato {
      background-color: tomato !important;
      color: #FFF !important;
    }

    .bg-dodgerblue {
      background-color: dodgerblue !important;
      color: #FFF !important;
    }

    .bg-violet {
      background-color: violet !important;
      color: #FFF !important;
    }

    .success-text {
      color: #82b33a;
    }

    .bg-danger {
      background-color: #FF7857 !important;
      color: #FFF !important;
    }

    .danger-text {
      color: #d15b3d;
    }

    .panel-box .panel-icon {
      display: table-cell;
      padding: px;
      width: 1%;
      vertical-align: top;
      border-radius: 0px;
      text-align: center;
      /*border-bottom: 1px solid #fcfcfc;
      border-top: 1px solid #fcfcfc;
      border-right: 1px solid #fcfcfc;*/
    }

    .panel-box .size-h1 {
      margin-top: 7px;
    }

    .panel-box .fa {
      margin-top: 9px;
    }

    .bg-lovender {
      background-color: #8075C4 !important;
      color: #FFF !important;
    }

    .lovender-text {
      color: #6a5fb1;
    }

    .capitalize {
      text-transform: capitalize;
    }

    .uppercase {
      text-transform: uppercase;
    }
  </style>
</head>
<body ng-app="myApp">
  <section id="container">
    <!-- header start-->
    <header class="header black-bg">
      <div class="sidebar-toggle-box">
        <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
      </div>
      <!-- logo start--><a class="logo" href="#"><b><img src="images/mobile.png" style="height:55px;width:100px;margin-top:-13px"></b></a>
      <!-- logo end-->
      <div class="top-menu">
        <ul class="nav pull-right top-menu">
          <li><a class="logout" href="/logout">Logout</a></li>
        </ul>
      </div>
    </header>
    <!-- header end-->
    <!-- sidebar start-->
    <aside>
      <div class="nav-collapse" id="sidebar">
        <!-- sidebar menu start-->
        <ul class="sidebar-menu" id="nav-accordion">
          <p class="centered"><a href="#"><i class="fa fa-user-circle" style="font-size:50px"></i></a></p>
          <h5 class="centered">
            <p>Student Dashboard</p>
          </h5>
          <li class="active"><a data-toggle="tab" href="#dashboard"><i class="fa fa-address-card"></i><span>Dashboard</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#education"><i class="fa fa-bars"></i><span>Education Details</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#manage"><i class="fa fa-bars"></i><span>Manage Details</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#vacancy"><i class="fa fa-bars"></i><span>View Vacancy</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#applied"><i class="fa fa-search"></i><span>Applied Jobs </span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#reports"><i class="fa fa-search"></i><span>Reports </span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#search"><i class="fa fa-search"></i><span>Search Job </span></a></li>
        </ul>
        <!-- sidebar menu end-->
      </div>
    </aside>
    <!-- sidebar end-->
    <!-- main content start-->
    <section id="main-content">
      <section class="wrapper site-min-height">
        <div class="row mt">
          <div class="col-md-12">
            <div class="tab-content">
              <div class="tab-pane fade in active" id="dashboard">
                <div class="row dashboard-stats container">
                  <div class="col-md-4 col-md-offset-1 col-sm-6">
                    <section class="panel panel-box">
                      <div class="panel-left panel-icon bg-success"><span style="font-size:30px">20</span></div>
                      <div class="panel-right panel-icon bg-reverse">
                        <p class="text-muted no-margin text">
                          <h3>Today Applied</h3>
                        </p>
                        <p class="size-h2"> </p>
                      </div>
                    </section>
                  </div>
                  <div class="col-md-4 col-md-offset-1 col-sm-6">
                    <section class="panel panel-box">
                      <div class="panel-left panel-icon bg-danger"><span style="font-size:30px">10</span></div>
                      <div class="panel-right panel-icon bg-reverse">
                        <p class="text-muted no-margin text">
                          <h3>Present Week</h3>
                        </p>
                        <p class="size-h2"> </p>
                      </div>
                    </section>
                  </div>
                  <div class="col-md-4 col-md-offset-1 col-sm-6">
                    <section class="panel panel-box">
                      <div class="panel-left panel-icon bg-lovender"><span style="font-size:30px">10</span></div>
                      <div class="panel-right panel-icon bg-reverse">
                        <p class="text-muted no-margin text">
                          <h3>Vacancy Count</h3>
                        </p>
                        <p class="size-h2"> </p>
                      </div>
                    </section>
                  </div>
                  <div class="col-md-4 col-md-offset-1 col-sm-6">
                    <section class="panel panel-box">
                      <div class="panel-left panel-icon bg-warning"><span style="font-size:30px">30</span></div>
                      <div class="panel-right panel-icon bg-reverse">
                        <p class="text-muted no-margin text">
                          <h3>Total Applied</h3>
                        </p>
                        <p class="size-h2"> </p>
                      </div>
                    </section>
                  </div>
                </div>
              </div>
              <div class="tab-pane fade align" id="education" ng-controller="educationController">
                <div class="container col-md-8 col-md-offset-2">
                  <form class="form-group">
                    <h1>Education Details</h1>
                      <label for="text">10th(secondary)</label>
                      <input type="text" class="form-control" id="name" placeholder="Board/university" ng-model="education.name">
                      <label for="number">Year</label>
                      <input type="year" class="form-control" id="year" placeholder="Year" ng-model="education.year">
                      <label for="text">Percentage</label>
                      <input type="number" class="form-control" id="Percentage" placeholder="Percentage"  ng-model="education.Percentage">
                      <label for="text">Cgpa</label>
                      <input type="number" class="form-control" id="Cgpa" placeholder="cgpa"  ng-model="education.Cgpa">
                      <label for="text">12th(senior secondary)</label>
                      <input type="text" class="form-control" id="name" placeholder="Board/university" ng-model="education.name">
                      <label for="number">Year</label>
                      <input type="year" class="form-control" id="year" placeholder="Year" ng-model="education.year">
                      <label for="text">Percentage</label>
                      <input type="number" class="form-control" id="Percentage" placeholder="Percentage"  ng-model="education.Percentage">
                      <label for="text">Cgpa</label>
                      <input type="number" class="form-control" id="Cgpa" placeholder="cgpa"  ng-model="education.Cgpa">
                      <label for="text">Graduation</label>
                      <input type="text" class="form-control" id="name" placeholder="Board/university" ng-model="education.name">
                      <label for="number">Year</label>
                      <input type="year" class="form-control" id="year" placeholder="Year" ng-model="education.year">
                      <label for="text">Percentage</label>
                      <input type="number" class="form-control" id="Percentage" placeholder="Percentage"  ng-model="education.Percentage">
                      <label for="text">Cgpa</label>
                      <input type="number" class="form-control" id="Cgpa" placeholder="cgpa"  ng-model="education.Cgpa">
                      <label for="text-area">Other Acheivements/Certifications/Qualification</label>
                      <input type="text" class="form-control" id="name"  ng-model="education.name">
                      <br>
                      <button type="Submit" class="btn btn-info" ng-click="studentData(education)">Submit</button>
              </form>
              </div>
              </div>
              <div class="tab-pane fade align" id="manage">
              </div>
              <div class="tab-pane fade align" id="vacancy">
              </div>
              <div class="tab-pane fade align" id="applied">
              </div>
              <div class="tab-pane fade align" id="reports">
              </div>
              <div class="tab-pane fade align" id="search">
              </div>
            </div>
          </div>
        </div>
      </section>
    </section>
  </section>
<script src="javascripts/jquery.js"></script>
<script src="javascripts/bootstrap.min.js"></script>
<script src="javascripts/jquery-ui-1.9.2.custom.min.js"></script>
<script src="javascripts/jquery.ui.touch-punch.min.js"></script>
<script src="javascripts/jquery.dcjqaccordion.2.7.js"></script>
<script src="javascripts/jquery.scrollTo.min.js"></script>
<script src="javascripts/jquery.nicescroll.js"></script>
<!-- common script for all pages-->
<script src="javascripts/common-scripts.js"></script>
</body>
</html>